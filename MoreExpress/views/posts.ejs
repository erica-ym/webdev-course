<% include partials/header %>

<h1>The Posts Page</h1>

<% for(var i = 0; i < posts.length; i++){ %>
 <li>
     <%= posts[i].title %> - <strong><%= posts[i].name %></strong>
 </li>
<% } %>

<!--does the same thing --> 
<% posts.forEach(function(post) { %>
    <li>
     <%= post.title %> - <strong><%= post.name %></strong>
 </li>
<% }) %>

<% include partials/footer %>